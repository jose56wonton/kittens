<h1>All Kittens</h1>
<% if @kittens %>
<table>
  <thead>
    <th>Name</th>
    <th>Age</th>
    <th>Cuteness</th>
    <th>Softness</th>
  </thead>
  <tbody>
    <% @kittens.each do |k| %>
    <tr>
      <%= render 'kittens/kitten', k:k %>
      <td>
         <%= link_to "Edit", edit_kitten_path(k.id)%>
      </td>
      <td>
         <%= link_to "Delete", kitten_path(k.id), method: :delete, data:{confirm: "That kitten is to young to die </3"}%>
      </td>
    </tr>
    <% end %>
  </tbody>

</table>
<% end %>
<h4><%= link_to "Generate", new_kitten_path %></h4>
